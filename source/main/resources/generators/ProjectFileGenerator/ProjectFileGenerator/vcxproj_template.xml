<Template>
  <!-- The blueprint -->
  <Configuration>
    <!-- StaticLibrary, DynamicLibrary, Application -->
    <ConfigurationType>StaticLibrary</ConfigurationType>
    <CharacterSet>NotSet</CharacterSet>
    <WholeProgramOptimization>false</WholeProgramOptimization>
    <UseDebugLibraries>true</UseDebugLibraries>
  </Configuration>
  <ImportGroup>
    <Import>
      <Condition>exists('$(UserRootDir)\Microsoft.Cpp.$(Platform).user.props')</Condition>
      <Project>$(UserRootDir)\Microsoft.Cpp.$(Platform).user.props</Project>
      <Label>LocalAppDataPlatform</Label>
    </Import>
  </ImportGroup>
  <OutDir>
    <Condition>'$(Configuration)|$(Platform)'=='%s'</Condition>
    <Value>$(ProjectName)_$(Configuration)_$(PlatformName)\</Value>
  </OutDir>
  <IntDir>
    <Condition>'$(Configuration)|$(Platform)'=='%s'</Condition>
    <Value>$(ProjectName)_$(Configuration)_$(PlatformName)\</Value>
  </IntDir>
  <TargetName>
    <Condition>'$(Configuration)|$(Platform)'=='%s'</Condition>
    <Value></Value>
  </TargetName>
  <IgnoreImportLibrary>
    <Condition>'$(Configuration)|$(Platform)'=='%s'</Condition>
    <Value></Value>
  </IgnoreImportLibrary>
  <GenerateManifest>
    <Condition>'$(Configuration)|$(Platform)'=='%s'</Condition>
    <Value>false</Value>
  </GenerateManifest>
  <LinkIncremental>
    <Condition>'$(Configuration)|$(Platform)'=='%s'</Condition>
    <Value>false</Value>
  </LinkIncremental>
  <ClCompile>
    <WarningLevel>Level3</WarningLevel>
    <Optimization>Disabled</Optimization>
    <PreprocessorDefinitions>%(PreprocessorDefinitions)</PreprocessorDefinitions>
    <PrecompiledHeader>NotUsing</PrecompiledHeader>
    <AdditionalIncludeDirectories>%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    <ExceptionHandling>false</ExceptionHandling>
    <FavorSizeOrSpeed>Size</FavorSizeOrSpeed>
    <RuntimeTypeInfo>false</RuntimeTypeInfo>
    <MinimalRebuild>true</MinimalRebuild>
  </ClCompile>
  <Link>
    <GenerateDebugInformation>true</GenerateDebugInformation>
    <AdditionalDependencies></AdditionalDependencies>
    <AdditionalLibraryDirectories></AdditionalLibraryDirectories>
    <SubSystem></SubSystem>
  </Link>
  <ResourceCompile>

  </ResourceCompile>
  <Lib>
    <OutputFile>$(OutDir)\$(TargetName)$(TargetExt)</OutputFile>
    <AdditionalOptions></AdditionalOptions>
  </Lib>

  <!-- Win32 will inherit all properties -->
  <Win32>
    <ClCompile>
      <PreprocessorDefinitions>
        <Insert>WIN32</Insert>
      </PreprocessorDefinitions>
    </ClCompile>

    <!-- Debug will inherit all properties -->
    <Debug>
      <ClCompile>
        <PreprocessorDefinitions>
          <Insert>DEBUG</Insert>
          <Insert>Target_Dev_Debug_Win32</Insert>
        </PreprocessorDefinitions>
      </ClCompile>
    </Debug>
    <Release>
      <ClCompile>
        <Optimization>O2</Optimization>
        <PreprocessorDefinitions>
          <Insert>NDEBUG</Insert>
          <Insert>Target_Dev_Release_Win32</Insert>
        </PreprocessorDefinitions>
      </ClCompile>
    </Release>
    <Profile>
      <ClCompile>
        <Optimization>O2</Optimization>
        <PreprocessorDefinitions>
          <Insert>NDEBUG</Insert>
          <Insert>Target_Dev_Profile_Win32</Insert>
        </PreprocessorDefinitions>
      </ClCompile>
    </Profile>
    <Final>
      <ClCompile>
        <Optimization>O3</Optimization>
        <PreprocessorDefinitions>
          <Insert>NDEBUG</Insert>
          <Insert>Target_Dev_Final_Win32</Insert>
        </PreprocessorDefinitions>
      </ClCompile>
      <Link>
        <GenerateDebugInformation>false</GenerateDebugInformation>
      </Link>
    </Final>
  </Win32>
  <NintendoDS>

  </NintendoDS>
  <NintendoWII>

  </NintendoWII>
  <Nintendo3DS>

  </Nintendo3DS>
  <SonyPSP>

  </SonyPSP>
  <SonyPS3>

  </SonyPS3>
</Template>
